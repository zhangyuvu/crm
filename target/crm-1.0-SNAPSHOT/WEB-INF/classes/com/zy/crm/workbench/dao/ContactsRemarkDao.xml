<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zy.crm.workbench.dao.ContactsRemarkDao">

    <insert id="insertContactsRemark"  >
         insert into tbl_contacts_remark(
            id,
            noteContent,
            createBy,
            createTime,
            editFlag,
            contactsId

        ) values (
            #{id},
            #{noteContent},
            #{createBy},
            #{createTime},
            #{editFlag},
            #{contactsId}
        )
    </insert>
</mapper>

































